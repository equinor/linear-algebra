{
  "private": false,
  "name": "@equinor/videx-linear-algebra",
  "version": "0.0.1-alpha",
  "description": "Math library for doing basic linear algebra operations",
  "repository": "https://github.com/equinor/videx-linear-algebra",
  "contributors": [
    "Kjerand Pedersen",
    "Tom Kristian Tjemsland"
  ],
  "license": "MIT",
  "keywords": [
    "vector",
    "math",
    "matrix",
    "matricies",
    "linear",
    "algebra",
    "transformations",
    "2d",
    "3d",
    "4d"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": "dist/linear-algebra.min.js",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "expect": "^24.9.0",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "rollup": "^1.24.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-uglify": "^6.0.3"
  },
  "scripts": {
    "build": "node node_modules/.bin/rollup -c",
    "dev": "node node_modules/.bin/rollup -c -w",
    "coverage": "NODE_ENV=test npx nyc --reporter=html --reporter=text mocha --require @babel/register",
    "test": "npx mocha --require @babel/register",
    "test:watch": "NODE_ENV=test npx mocha --require @babel/register --reporter min --watch",
    "lint": "npx eslint src test tools",
    "prepub": "npm run build",
    "pub": "npm publish --access=public",
    "docs": "./node_modules/.bin/esdoc"
  },
  "files": [
    "dist"
  ]
